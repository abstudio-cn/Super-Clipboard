<UserControl x:Class="superClipboard.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:superClipboard"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <ui:SymbolIcon Symbol="Settings24" FontSize="48" Margin="0,0,0,20" Foreground="White"/>
            <TextBlock Text="设置页面" FontSize="24" FontWeight="Bold" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock Text="应用程序设置" FontSize="16" Margin="0,10,0,0" HorizontalAlignment="Center" Foreground="White"/>
        </StackPanel>
    </Grid>
</UserControl>
